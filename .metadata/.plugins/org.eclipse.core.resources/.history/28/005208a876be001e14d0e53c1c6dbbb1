package ch03;      //24.01.29 월

// 제너릭 : 클래스나 메서드에서 사용할 데이터 타입을 외부에서 받아오는 기법
class Test1 {
	String subject;
	int score;
}

class Test2 {
	String subject;
	String score;
}

public class C_Generic {

	public static void main(String[] args) {
		
		Test1 myTest = new Test1 ();
		myTest.subject = "국어";
		myTest.score = 95;	
		
		Test2 yourTest = new Test2();
		yourTest.subject = "경영학원론";
		yourTest.score = "A+";
				
		
	}

}
